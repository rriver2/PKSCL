                                                                                                                                                      \n                                                                                                                                                                <input type=\"text\" style={{ border: \"transparent\", textAlign: \"center\" ,width:\"90px\" }} \n                                                                                                                                                                placeholder={item[\"price\"]}\n                                                                                                                                                                value={item[\"price\"]}\n                                                                                                                                                                    onInput={\n                                                                                                                                                                        (e) => {\n                                                                                                                                                                            changeItem(\"price\", e.target.value.replace(/[^0-9]/g, ''), j, k);\n                                                                                                                                                                        }}></input>\n                                                                                                                                                                \n                                                                                                                                                    </td>\n\n                                                                                                                        